<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:name=".Activites.MyIteams"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_splash"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_splash"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel_planer"
        tools:targetApi="31">
        <activity
            android:name=".Activites.ForgetActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.ReceiptActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.ticketActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.BookActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.CategoryDetails"
            android:exported="false" />
        <activity
            android:name=".Activites.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activites.SignupActivity"
            android:exported="false" />
        <activity
            android:name=".Activites.MainActivity"
            android:exported="false" />
    </application>

</manifest>